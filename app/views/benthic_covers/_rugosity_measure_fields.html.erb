<div class="span3">
  <fieldset>
    <div class="control-group">
      <label class="control-label">Min Depth(ft)</label>
      <div class="controls">
        <%= f.text_field :min_depth %>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label">Max Depth(ft)</label>
      <div class="controls">
        <%= f.text_field :max_depth %>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label">Max Vert. Height(m)</label>
      <div class="controls">
        <%= f.text_field :max_vert_height %>
      </div>
    </div>
  </fieldset>
</div>

<div class="span3">
  <fieldset>
    <div class="control-group">
      <label class="control-label"> 0 - 19cm </label>
      <div class="controls">
        <%= f.text_field :cnt_less_than_20%>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label"> 20 - 49cm </label>
      <div class="controls">
        <%= f.text_field :cnt_20_less_than_50 %>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label"> 50 - 99cm </label>
      <div class="controls">
        <%= f.text_field :cnt_50_less_than_100 %>
      </div>
    </div>




    <div class="control-group">
      <label class="control-label"> 100 - 149cm </label>
      <div class="controls">
        <%= f.text_field :cnt_100_less_than_150%>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label"> 150 - 199cm </label>
      <div class="controls">
        <%= f.text_field :cnt_150_less_than_200 %>
      </div>
    </div>

    <div class="control-group">
      <label class="control-label"> &ge; 200cm </label>
      <div class="controls">
        <%= f.text_field :cnt_greater_than_200 %>
      </div>
    </div>
  </fieldset>
</div>
